    "85272ee1-d7ec-498c-985b-6761fcc6f38f": {
      "type": "image-with-text",
      "blocks": {
        "template--15144309456939__85272ee1-d7ec-498c-985b-6761fcc6f38f-1684401872db8a5321-0": {
