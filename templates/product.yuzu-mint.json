    "91480b05-d41b-49d7-86d2-128865d27814": {
      "type": "image-with-text",
      "blocks": {
        "template--15144309456939__91480b05-d41b-49d7-86d2-128865d27814-1685613467f4e1b0e4-0": {
          "type": "heading",
          "settings": {
            "heading": "Our Science",
